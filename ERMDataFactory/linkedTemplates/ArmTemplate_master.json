{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "ERMDataFactory"
		},
		"AzureBlobSinkTransform_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobSinkTransform'"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureBlobStorage2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage2'"
		},
		"OutputLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'OutputLinkedService'"
		},
		"TestSink_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'TestSink'"
		},
		"azuredestinationlinkedservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azuredestinationlinkedservice'"
		},
		"azuresourcelinkedservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azuresourcelinkedservice'"
		},
		"AzureBlobJson_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"AzureBlobJson_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "ermblob"
		},
		"AzureBlobStorageSourceDatasetEntity1_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"AzureBlobStorageSourceDatasetEntity1_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "ermblob"
		},
		"triggerWhenFileInBlobStorage_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/7929cc89-eab0-4bb8-bec5-a5c6f38494fb/resourceGroups/ERM-ETL/providers/Microsoft.Storage/storageAccounts/ermsourceblob"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobSinkTransform_connectionString": {
						"value": "[parameters('AzureBlobSinkTransform_connectionString')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureBlobStorage2_connectionString": {
						"value": "[parameters('AzureBlobStorage2_connectionString')]"
					},
					"OutputLinkedService_connectionString": {
						"value": "[parameters('OutputLinkedService_connectionString')]"
					},
					"TestSink_connectionString": {
						"value": "[parameters('TestSink_connectionString')]"
					},
					"azuredestinationlinkedservice_connectionString": {
						"value": "[parameters('azuredestinationlinkedservice_connectionString')]"
					},
					"azuresourcelinkedservice_connectionString": {
						"value": "[parameters('azuresourcelinkedservice_connectionString')]"
					},
					"AzureBlobJson_properties_typeProperties_fileName": {
						"value": "[parameters('AzureBlobJson_properties_typeProperties_fileName')]"
					},
					"AzureBlobJson_properties_typeProperties_folderPath": {
						"value": "[parameters('AzureBlobJson_properties_typeProperties_folderPath')]"
					},
					"AzureBlobStorageSourceDatasetEntity1_properties_typeProperties_fileName": {
						"value": "[parameters('AzureBlobStorageSourceDatasetEntity1_properties_typeProperties_fileName')]"
					},
					"AzureBlobStorageSourceDatasetEntity1_properties_typeProperties_folderPath": {
						"value": "[parameters('AzureBlobStorageSourceDatasetEntity1_properties_typeProperties_folderPath')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"triggerWhenFileInBlobStorage_properties_typeProperties_scope": {
						"value": "[parameters('triggerWhenFileInBlobStorage_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		}
	]
}